<!-- 
  Navigation
-->

<launch>
  <include file="$(find kait_navigation)/launch/amcl.launch" />

 
  <!--- 経路計画，指令値生成ノード　move_base -->
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" >
    <rosparam file="$(find mega_vlp_slam)/params/costmap_common.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find mega_vlp_slam)/params/costmap_common.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find mega_vlp_slam)/params/local_costmap.yaml" command="load" />
    <rosparam file="$(find mega_vlp_slam)/params/global_costmap.yaml" command="load" />
    <rosparam file="$(find mega_vlp_slam)/params/dwa_planner.yaml" command="load" />
    <rosparam file="$(find mega_vlp_slam)/params/move_base.yaml" command="load" />
  </node>

</launch>

